{
  "name": "front-end-typescript",
  "version": "1.0.0",
  "main": "server.mjs",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server-prod": "node server.mjs",
    "server-dev": "yarn nodemon server.mjs",
    "client": "yarn start --prefix ./frontend",
    "dev": "concurrently --kill-others-on-fail \"yarn server-dev\" \"yarn client\"",
    "start": "concurrently --kill-others-on-fail \"yarn server-dev\" \"yarn client\"",
    "heroku-postbuild": "cd frontend && yarn install && yarn build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node": "^21.2.0",
    "vite": "^5.0.10",
    "@vitejs/plugin-react": "^4.2.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": "1.22.x"
  }
}
