    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "vite": "^5.0.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
